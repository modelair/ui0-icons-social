    //    //    "src/renderComponent/*.vue",
    //    "src/renderComponent/icons/*.vue",
    //    "src/renderComponent/value.ts",
    //    "src/util/*",
    //    "src/**/*.d.ts",
    //    "src/**/*.tsx",
    //    "src/**/*.vue",
    //    "src/index.ts",
    //    "types.d.ts"
    //  ],
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "vite.d.ts",
    //        "rollup.config.ts",
//    "../core/types.d.ts"
  ]
}
